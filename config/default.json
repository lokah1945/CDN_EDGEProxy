{
  "cache": {
    "maxSizeBytes": 2199023255552,
    "maxAgeMs": 86400000,
    "directory": "./data/cdn-cache",
    "revalidateStale": true
  },
  "profiles": {
    "chromium": "./data/profiles/chromium",
    "chrome": "./data/profiles/chrome",
    "msedge": "./data/profiles/msedge",
    "firefox": "./data/profiles/firefox"
  },
  "routing": {
    "serviceWorkers": "block",
    "bypassMethods": ["POST", "PUT", "DELETE", "PATCH"],
    "bypassResourceTypes": ["websocket"],
    "beaconMaxBytes": 4096,
    "beaconPatterns": [
      "pixel", "beacon", "collect", "impression", "imp",
      "view", "track", "ping", "log", "event", "hit"
    ],
    "cacheableContentTypes": [
      "image/", "video/", "audio/", "font/",
      "application/javascript", "text/css",
      "application/wasm", "application/octet-stream"
    ],
    "adNetworkPatterns": [
      "doubleclick.net", "googlesyndication.com", "googleadservices.com",
      "adnxs.com", "criteo.com", "facebook.com/tr", "amazon-adsystem.com",
      "adsrvr.org", "rubiconproject.com", "pubmatic.com", "openx.net",
      "casalemedia.com", "indexww.com", "taboola.com", "outbrain.com",
      "bidswitch.net", "smartadserver.com", "yieldmo.com"
    ]
  }
}
